{
  "name": "require-extension-hooks-vue",
  "version": "2.0.0",
  "description": "Simple server parser for .vue files",
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "lint": "eslint src"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js"
    ],
    "source": [
      "index.js"
    ],
    "require": [
      "./spec/helpers/setup.js"
    ],
    "verbose": false,
    "concurrency": 8,
    "timeout": "240s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackmellis/require-extension-hooks-vue.git"
  },
  "author": "Jack Ellis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackmellis/require-extension-hooks-vue/issues"
  },
  "homepage": "https://github.com/jackmellis/require-extension-hooks-vue#readme",
  "peerDependencies": {
    "require-extension-hooks": "^0.3.0"
  },
  "dependencies": {
    "@vue/component-compiler-utils": "^2.3.1",
    "consolidate": "^0.15.0",
    "postcss": "^7.0.0",
    "postcss-modules-sync": "^1.0.0",
    "source-map-support": "^0.5.8",
    "vue-template-compiler": "^2.5.22"
  },
  "devDependencies": {
    "browser-env": "^3.2.5",
    "ava": "^0.25.0",
    "eslint": "^5.0.0",
    "pug": "^2.0.3",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.0",
    "sinon": "^6.0.0",
    "vue": "^2.5.22"
  }
}
